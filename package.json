{
  "name": "tree-sitter-fastbuild",
  "version": "0.0.1",
  "description": "FASTBuild grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.21.0"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.fastbuild",
      "file-types": [
        "bff"
      ],
      "first-line-regex": "#!.*\\bfbuild$"
    }
  ]
}
