{
  "grammars": [
    {
      "name": "fastbuild",
      "camelcase": "Fastbuild",
      "scope": "source.fastbuild",
      "path": ".",
      "file-types": [
        "bff"
      ],
      "first-line-regex": "#!.*\\bfbuild$"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "FASTBuild grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fastbuild"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
